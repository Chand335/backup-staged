{
    "name": "backup-staged/laravel-backup-staged",
    "description": "Laravel package that backs up staged git files into timestamped archives.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/filesystem": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "symfony/process": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "BackupStaged\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BackupStaged\\BackupStagedServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
